         <br >

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
    <div>
      <%= image_tag 'login/dashboard_login_icon.png' %>
      </div>

    <br >

    <div>
      <%= image_tag 'login/dashboard_login_user.png' %>
      <%= f.email_field :email, :autofocus => true, :placeholder => 'Email Address' %>
    </div>

    <div>
      <%= image_tag 'login/dashboard_login_password.png' %>
      <%= f.password_field :password, :placeholder => 'Password' %>
    </div>

    <div><%= f.submit "Sign in" %></div>
    <div><%= link_to 'Don\'t have an account? Sign up here', signup_path %></div>
<% end %>

<%= render "devise/shared/links" %>
