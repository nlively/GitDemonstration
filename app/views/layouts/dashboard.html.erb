<!DOCTYPE html>
<html>
<head>
  <title>OauthServer</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="top-bar">
  <% unless current_user.nil? or current_user.agency.nil? %>
      You are signed in to <%= current_user.agency.name %>
  <% end %>
</div>

<div id="page">
  <div id="page-inner">
    <div id="header" class="">
      <div id="header-inner">
        <%= render 'layouts/header' %>
      </div>
    </div>

    <div id="main" class="">
      <div id="main-inner">

        <div id="navigation">



          <%= render 'layouts/dashboard_top_menu' %>
        </div>

        <p class="notice"><%= notice %></p>
        <p class="alert"><%= alert %></p>

        <div id="sidebar-first" class="sidebar">
          <div id="sidebar-first-inner" class="sidebar-inner">
            <%= render 'layouts/dashboard_sidebar_menu' %>
          </div>
        </div>

        <div id="content">
          <div id="content-inner">
            <%= yield %>
          </div>
        </div>

      </div>
    </div>

    <div id="footer" class="">
      <div id="footer-inner">
      </div>
    </div>


    <%= render 'layouts/copyright' %>

    <%=  render 'layouts/tracking' %>


  </div>
</div>


</body>
</html>
