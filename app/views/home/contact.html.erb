<%= form_for @contact_submission, :url => contact_path, :method => :post do |f| %>

    <div class="form-field">
      <div class="label">
        <%= f.label :name, 'Name' %>
      </div>
      <div class="field">
        <%= f.text_field :name %>
      </div>
    </div>

    <div class="form-field">
      <div class="label">
        <%= f.label :agency_name, 'Agency Name' %>
      </div>
      <div class="field">
        <%= f.text_field :agency_name %>
      </div>
    </div>

    <div class="form-field">
      <div class="label">
        <%= f.label :email, 'Email Address' %>
      </div>
      <div class="field">
        <%= f.email_field :email %>
      </div>
    </div>

    <div class="form-field">
      <div class="label">
        <%= f.label :message, 'Message' %>
      </div>
      <div class="field">
        <%= f.text_area :message %>
      </div>
    </div>


    <%= f.submit 'Send Message' %>
<% end %>