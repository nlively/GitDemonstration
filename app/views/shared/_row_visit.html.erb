<tr class="visit-row editable" data-type="<%= type %>" date-remote="true" data-id="<%= visit.id %>">
  <td class="employee" rel="<%= visit.user_id %>">
    <%= visit.employee_label %>
  </td>
  <td class="payrate">
    <%= visit.pay_rate%>
  </td>
  <td class="client" rel="<%= visit.care_recipient_id %>">
    <%= visit.client_label %>
  </td>
  <td class="billrate">
    <%= visit.bill_rate%>
  </td>
  <td class="in-time">
    <%= visit.in_time_formatted %>
  </td>
  <% if type == :completed or type == 'completed' %>
      <td class="out-time">
        <%= visit.out_time_formatted %>
      </td>
      <td class="duration">
        <%= visit.duration_string %>
      </td>
  <% end %>
  <td class="location">
    <%= visit.location_label %>
  </td>
  <% if type == :completed or type == 'completed' %>
      <td class="approve">
        <%= check_box_tag :approve %>
      </td>
  <% end %>
</tr>